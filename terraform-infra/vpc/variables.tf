# Define the AWS Access Key
variable "access_key" {
  # Replace with your AWS Access Key
  default = "<access_key>"
}

# Define the AWS Secret Key
variable "secret_key" {
  # Replace with your AWS Secret Key
  default = "<secret_key>"
}

# Define the AWS Region
variable "region" {
  # Replace with the AWS region you want to deploy your resources to
  default = "<replace_with_your_region>"
}